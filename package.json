{
  "name": "node-ts-books-db",
  "version": "1.0.0",
  "description": "Hola este es el repo de node-ts-books-db",
  "main": "index.ts",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install",
    "seed:books": "node ./dist/seeds/book.seed.js",
    "seed:authors": "node ./dist/seeds/author.seed.js",
    "seed:relations": "node ./dist/seeds/relations.seed.js",
    "seed:all": "npm run seed:books && npm run seed:authors && npm run seed:relations",
    "normalization:book": "node ./normalizations/book.normalization.js",
    "normalization:author": "node ./normalizations/author.normalization.js",
    "seed:course-and-student": "ts-node-dev ./src/seeds/course-and-student.seed.ts --ignore-watch",
    "start": "ts-node-dev --inspect -- ./src/index.ts",
    "start:pro": "npm run build && node ./dist/index.js",
    "build": "tsc"
  },
  "keywords": [],
  "author": "Alejandro Brasoveanu",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.7",
    "@types/validator": "^13.7.17",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "eslint": "^8.41.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^5.3.0",
    "mongoose": "^7.0.5",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.3.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.16",
    "validator": "^13.9.0"
  }
}
